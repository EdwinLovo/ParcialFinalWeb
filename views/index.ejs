<body>
  
  <h2 class="mb-4">Administrador de Bebidas Alcoholicas</h2>

  <form action="" method="POST" id="form1" name="form1" class="form-inline">
    <section class="form-group mb-2">
      <input type="text" name="marca" id="marca" placeholder="Marca"  class="form-control">
    </section>
    <section class="form-group mx-sm-3 mb-2">
      <input type="text" name="tipo" id="tipo" placeholder="tipo"  class="form-control">
    </section>
    <section class="form-group mb-2">
      <input type="text" name="anios" id="anios" placeholder="Años" class="form-control">
    </section>
    <section class="mb-2 ml-4">
      <input type="submit" value="Guardar Producto" class="btn btn-primary">
    </section>
  </form>

  <table id="tabla" class="table table-hover mt-5">
    <thead class="thead-dark">
      <tr>
        <th>ID</th>
        <th>Marca</th>
        <th>Tipo</th>
        <th>Años</th>
        <th>Opciones</th>
      </tr>
    </thead>
    <tbody id="llenar"></tbody>
  </table>

  <!-- Button trigger modal 
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">
